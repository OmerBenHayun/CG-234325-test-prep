\end_inset


\begin_inset Formula 
\[
\tilde{l}=P\left(\begin{array}{c}
p_{x}+tv_{x}\\
p_{y}+tv_{y}\\
p_{z}+tv_{z}\\
1
\end{array}\right)=\left(\begin{array}{c}
\frac{1}{4}\left(p_{x}+tv_{x}\right)\\
\frac{1}{3}\left(p_{y}+tv_{y}\right)\\
-7\left(p_{z}+tv_{z}\right)\\
-\left(p_{z}+tv_{z}\right)
\end{array}\right)=\left(\begin{array}{c}
\frac{1}{4}\frac{\left(p_{x}+tv_{x}\right)}{\left(p_{z}+tv_{z}\right)}\\
\frac{1}{3}\frac{\left(p_{y}+tv_{y}\right)}{\left(p_{z}+tv_{z}\right)}\\
-7\\
-1
\end{array}\right)=\left(\begin{array}{c}
\frac{1}{4}\frac{\left(p_{x}+tv_{x}\right)}{\left(p_{z}+tv_{z}\right)}\\
\frac{1}{3}\frac{\left(p_{y}+tv_{y}\right)}{\left(p_{z}+tv_{z}\right)}\\
-7\\
-1
\end{array}\right)
\]

\end_inset


\end_layout

