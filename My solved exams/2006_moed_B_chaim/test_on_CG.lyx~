\noun default
\color inherit
הווקטורים שמצאנו
\begin_inset Formula 
\begin{align*}
\left(\tilde{d},\tilde{e},\tilde{f}\right) & =\left(a,b,c\right)\times\left(g,h,i\right)\\
 & =\left|\begin{array}{ccc}
\hat{i} & \hat{j} & \hat{k}\\
a & b & c\\
g & h & i
\end{array}\right|\\
 & =\hat{i}\left|\begin{array}{cc}
b & c\\
h & i
\end{array}\right|-\hat{j}\left|\begin{array}{cc}
a & c\\
g & i
\end{array}\right|+\hat{k}\left|\begin{array}{cc}
a & b\\
g & h
\end{array}\right|\\
 & =\hat{i}\left(bi-ch\right)-\hat{j}\left(ai-gc\right)+\hat{k}\left(ah-gb\right)\\
 & =\hat{i}\left(bi-\cancel{ch}\right)-\hat{j}\left(ai-\cancel{gc}\right)+\hat{k}\left(ah-gb\right)\\
 & =\hat{i}\left(bi\right)-\hat{j}\left(ai\right)+\hat{k}\left(ah-gb\right)\\
 & =\hat{i}\left(\frac{-A}{\sqrt{A^{2}+B^{2}}}\cdot\frac{C}{\sqrt{A^{2}+B^{2}+C^{2}}}\right)-\hat{j}\left(\frac{B}{\sqrt{A^{2}+B^{2}}}\cdot\frac{C}{\sqrt{A^{2}+B^{2}+C^{2}}}\right)+\hat{k}\left(\frac{B}{\sqrt{A^{2}+B^{2}}}\cdot\frac{B}{\sqrt{A^{2}+B^{2}+C^{2}}}-\frac{A}{\sqrt{A^{2}+B^{2}+C^{2}}}\cdot\frac{-A}{\sqrt{A^{2}+B^{2}}}\right)\\
 & =\hat{i}\left(\frac{-AC}{\sqrt{A^{2}+B^{2}}\sqrt{A^{2}+B^{2}+C^{2}}}\right)-\hat{j}\left(\frac{BC}{\sqrt{A^{2}+B^{2}}\sqrt{A^{2}+B^{2}+C^{2}}}\right)+\hat{k}\left(\frac{B^{2}+A^{2}}{\sqrt{A^{2}+B^{2}}\sqrt{A^{2}+B^{2}+C^{2}}}\right)\\
 & =\left(\frac{-AC}{\sqrt{A^{2}+B^{2}}\sqrt{A^{2}+B^{2}+C^{2}}},\frac{-BC}{\sqrt{A^{2}+B^{2}}\sqrt{A^{2}+B^{2}+C^{2}}},\frac{B^{2}+A^{2}}{\sqrt{A^{2}+B^{2}}\sqrt{A^{2}+B^{2}+C^{2}}}\right)
\end{align*}

\end_inset

ולכן הווקטור המנורמל הוא 
\begin_inset Formula 
\[
\left(d,e,f\right)=\left(\frac{-AC}{\sqrt{\left(AC\right)^{2}+\left(BC\right)^{2}+\left(B^{2}+A^{2}\right)^{2}}},\frac{-BC}{\sqrt{\left(AC\right)^{2}+\left(BC\right)^{2}+\left(B^{2}+A^{2}\right)^{2}}},\frac{B^{2}+A^{2}}{\sqrt{\left(AC\right)^{2}+\left(BC\right)^{2}+\left(B^{2}+A^{2}\right)^{2}}}\right)
\]

\end_inset


\end_layout

