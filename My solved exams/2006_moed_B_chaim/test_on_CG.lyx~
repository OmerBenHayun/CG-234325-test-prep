\end_inset

 נקבל כי 
\begin_inset Formula 
\[
l_{1}=m_{1}+tv\Rightarrow\tilde{v}_{1}\Rightarrow\left(\begin{array}{c}
-\frac{\left(m_{x}+tv_{x}\right)}{\left(m_{z}+tv_{z}\right)}\left(\frac{2\text{near}}{\text{ right }-\text{ left }}\right)\\
-\frac{\left(m_{y}+tv_{y}\right)}{\left(m_{z}+tv_{z}\right)}\left(\frac{\text{ 2near }}{\text{ top }-\text{ bottom }}\right)\\
0
\end{array}\right)=\left(\begin{array}{c}
-\frac{\left(1+tv_{x}\right)}{\left(0+t\right)}\left(\frac{2\text{near}}{\text{ right }-\text{ left }}\right)\\
-\frac{\left(0+tv_{y}\right)}{t}\left(\frac{\text{ 2near }}{\text{ top }-\text{ bottom }}\right)\\
0
\end{array}\right)=\left(\begin{array}{c}
-\left(\frac{1}{t}+1\right)\left(\frac{2\text{near}}{\text{ right }-\text{ left }}\right)\\
-\left(\frac{\text{ 2near }}{\text{ top }-\text{ bottom }}\right)\\
0
\end{array}\right)
\]

\end_inset


\begin_inset Formula 
\[
