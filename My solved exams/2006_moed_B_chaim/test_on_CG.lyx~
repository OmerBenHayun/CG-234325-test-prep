 הקווים המקבילים ייפגשו בנקודה 
\begin_inset Note Note
status open

\begin_layout Plain Layout
\begin_inset Formula 
\[
\left(\begin{array}{c}
-\left(\frac{\text{ right }+\text{ left }}{\text{ right }-\text{ left }}\right)-\left(\frac{2\text{near}}{\text{ right }-\text{ left }}\right)\\
-\left(\frac{\text{ top }+\text{ bottom }}{\text{ top }-\text{ bottom }}\right)-\left(\frac{\text{ 2near }}{\text{ top }-\text{ bottom }}\right)\\
\frac{\text{ far }+\text{ near }}{\text{ far }-\text{ near }}+\frac{2*\text{ near }*\text{ far }}{\text{ far }-\text{ near }}
\end{array}\right)=\left(\begin{array}{c}
\frac{-2\text{near}\text{- right }-\text{ left }}{\text{ right }-\text{ left }}\\
\left(\frac{-2\text{near}-\text{ top }-\text{ bottom }}{\text{ top }-\text{ bottom }}\right)\\
\frac{\left(\text{ far }+\text{ near }\right)^{2}}{\text{ far }-\text{ near }}
\end{array}\right)
\]

\end_inset


\end_layout

\end_inset


\begin_inset Formula 
\[
\left(\begin{array}{c}
-\left(\frac{\text{ right }+\text{ left }}{\text{ right }-\text{ left }}\right)-\left(\frac{2\text{near}}{\text{ right }-\text{ left }}\right)\\
-\left(\frac{\text{ top }+\text{ bottom }}{\text{ top }-\text{ bottom }}\right)-\left(\frac{\text{ 2near }}{\text{ top }-\text{ bottom }}\right)\\
