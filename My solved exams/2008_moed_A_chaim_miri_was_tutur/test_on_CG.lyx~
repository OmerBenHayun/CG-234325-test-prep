\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
\left(1,1,-1\right)\times\left(1,-1,-1\right) & =\left|\begin{array}{ccc}
i & j & k\\
1 & 1 & -1\\
1 & -1 & -1
\end{array}\right|\\
 & =i\left|\begin{array}{cc}
1 & -1\\
-1 & -1
\end{array}\right|-j\left|\begin{array}{cc}
1 & -1\\
1 & -1
\end{array}\right|+k\left|\begin{array}{cc}
1 & 1\\
1 & -1
\end{array}\right|\\
 & =i\left(-1-1\right)-j\left(-1-\left(-1\right)\right)+k\left(-1-1\right)\\
 & =-2i-2k\\
 & =\left(-2,0,-2\right)
\end{align*}

\end_inset

לאחר נרמול
\end_layout

