\begin_layout Standard
\begin_inset Box Boxed
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "100col%"
special "none"
height "1in"
height_special "totalheight"
thickness "0.4pt"
separation "3pt"
shadowsize "4pt"
framecolor "black"
backgroundcolor "none"
status collapsed

\begin_layout Plain Layout
חישוב הנורמל ייראה:
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
\left(2,1,-1\right)\times\left(2,-1,-1\right) & =\left|\begin{array}{ccc}
i & j & k\\
2 & 1 & -1\\
2 & -1 & -1
\end{array}\right|\\
 & =i\left|\begin{array}{cc}
1 & -1\\
-1 & -1
\end{array}\right|-j\left|\begin{array}{cc}
2 & -1\\
2 & -1
\end{array}\right|+k\left|\begin{array}{cc}
2 & 1\\
2 & -1
\end{array}\right|\\
 & =i\left(-1-1\right)-j\left(-2-\left(-2\right)\right)+k\left(-2-2\right)\\
 & =-2i-4k\\
 & =\left(-2,0,-4\right)
\end{align*}

\end_inset

נרמול 
\begin_inset Formula 
\[
\frac{\left(-2,0,-4\right)}{\sqrt{4+16}}=\frac{\left(-2,0,-4\right)}{\sqrt{20}}=-\frac{\left(\sqrt{4},0,\sqrt{16}\right)}{\sqrt{20}}=-\left(\sqrt{\frac{1}{5}},0,\sqrt{\frac{4}{5}}\right)=\left(-\sqrt{\frac{1}{5}},0,-\sqrt{\frac{4}{5}}\right)
\]

\end_inset


\end_layout

\begin_layout Plain Layout
לאחר נרמול 
\begin_inset Formula 
\[
\left(-\sqrt{\frac{1}{5}},0,-\sqrt{\frac{4}{5}}\right)
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
החישוב של הנורמל לפי השיטה שבסעיף ב':
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n'=\frac{T\left(\begin{array}{c}
-\frac{1}{\sqrt{2}}\\
0\\
-\frac{1}{\sqrt{2}}\\
1
\end{array}\right)}{\left\Vert T\left(\begin{array}{c}
-\frac{1}{\sqrt{2}}\\
0\\
-\frac{1}{\sqrt{2}}\\
1
\end{array}\right)\right\Vert }=\frac{\left(\begin{array}{c}
-2\frac{1}{\sqrt{2}}\\
0\\
-\frac{1}{\sqrt{2}}\\
1
\end{array}\right)}{\left\Vert \left(\begin{array}{c}
-2\frac{1}{\sqrt{2}}\\
0\\
-\frac{1}{\sqrt{2}}\\
1
\end{array}\right)\right\Vert }=\left(\begin{array}{c}
-\sqrt{\frac{4}{5}}\\
0\\
-\sqrt{\frac{1}{5}}\\
1
\end{array}\right)=\left(\begin{array}{c}
-\sqrt{\frac{4}{5}}\\
0\\
-\sqrt{\frac{1}{5}}
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
נבחין כי 
\begin_inset Formula $\left(\begin{array}{c}
-\sqrt{\frac{4}{5}}\\
0\\
-\sqrt{\frac{1}{5}}
\end{array}\right)\neq\left(-\sqrt{\frac{1}{5}},0,-\sqrt{\frac{4}{5}}\right)^{T}$
\end_inset

 ולכן 
\begin_inset Formula $n'\neq\bar{n}$
\end_inset

 ולכן החישוב הנ
\begin_inset Quotes erd
\end_inset

ל שגוי.
\end_layout

