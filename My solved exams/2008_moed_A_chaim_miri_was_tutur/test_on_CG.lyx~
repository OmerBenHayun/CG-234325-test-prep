\shape default
\size default
\emph default
\bar default
\strikeout default
\xout default
\uuline default
\uwave default
\noun default
\color inherit
נקודות לנורמל במישור 
\begin_inset Formula $p_{1},p_{2}$
\end_inset

 הנורמל 
\begin_inset Formula $n$
\end_inset

 אליהם מקיים 
\begin_inset Formula 
\[
\star:n^{T}\left(p_{1}-p_{2}\right)=0
\]

\end_inset

לאחר הטרנס נסמן 
\begin_inset Formula $\forall i\in\left\{ 1,2\right\} :\tilde{p}_{i}=T\left(p+i\right)=Ap_{i}+b$
\end_inset

 ונסמן 
\begin_inset Formula $\tilde{n}$
\end_inset

 כהנומל שלאחר הטרס.
\end_layout

\begin_layout Standard
נדרוש שייתקיים 
\begin_inset Formula 
\[
\tilde{n}^{T}\left(\tilde{p}_{1}-\tilde{p}_{2}\right)=0
\]

\end_inset

נציב את
\begin_inset Formula $\forall i\in\left\{ 1,2\right\} :\tilde{p}_{i}$
\end_inset

 ונקבל 
\begin_inset Formula 
\[
\tilde{n}^{T}\left(Ap_{1}+b-\left(Ap_{2}+b\right)\right)=0
\]

\end_inset

 כלומר מתקיים 
\begin_inset Formula 
\[
\tilde{n}^{T}\left(\left(Ap_{1}-Ap_{2}\right)\right)=0
\]

\end_inset

נסמן את הטרנס שצריך לעבור הנורמל 
\begin_inset Formula $B$
\end_inset

 כלומר 
\begin_inset Formula $\tilde{n}=Bn$
\end_inset

 המטרה שלנו למצוא את 
\begin_inset Formula $B$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
הנחה:
\series default
 
\begin_inset Formula $A$
\end_inset

 היא הפיכה.
\end_layout

\begin_layout Standard
אזי 
\end_layout

\begin_layout Standard
עמ שייתיקים 
\begin_inset Formula $\tilde{n}^{T}\left(\left(Ap_{1}-Ap_{2}\right)\right)=0$
\end_inset

נדרוש כי 
\begin_inset Formula 
\[
\left(Bn\right)^{T}\left(\left(Ap_{1}-Ap_{2}\right)\right)=0
\]

\end_inset

כלומר 
\begin_inset Formula 
\[
nB^{T}\left(\left(Ap_{1}-Ap_{2}\right)\right)=0
\]

\end_inset

כלומר 
\begin_inset Formula 
\[
n\left(B^{T}Ap_{1}-B^{T}Ap_{2}\right)=0
\]

\end_inset

אם נדרוש כי 
\begin_inset Formula $\boxed{B^{T}=A^{-1}}$
\end_inset

 נקבל כי מתקיים 
\begin_inset Formula 
\[
n\left(\unote{B^{T}A}Ip_{1}-\unote{B^{T}A}IAp_{2}\right)=0
\]

\end_inset

 כלומר 
\begin_inset Formula $n\left(p_{1}-p_{2}\right)=0$
\end_inset

 וזה נכון לפטי 
\begin_inset Formula $\star$
\end_inset

.
\end_layout

\begin_layout Standard
לכן בהנתן ש
\begin_inset Formula $A$
\end_inset

 הפיכה )תנאי שקול לכך במונחי השאלה : בהנתן שטרנס' 
\begin_inset Formula $T$
\end_inset

  הפיכה( נדרוש כי 
\begin_inset Formula $B^{T}=A^{-1}$
\end_inset

 כלומר ש 
\begin_inset Formula $B=\left(A^{-1}\right)^{T}$
\end_inset

 כלומר נדרוש להכפיל את המט' של הנורמל ב
\begin_inset Formula $B$
\end_inset

 על מנת שהנ
\begin_inset Quotes erd
\end_inset

ל מתקיים.
\begin_inset Formula 
\[
\tilde{n}=Bn
\]

\end_inset

כלומר הרטנס היא 
\begin_inset Formula 
\[
\tilde{n}=Bn
\]

\end_inset


\end_layout

\begin_layout Standard
כאשר 
\begin_inset Formula $B=\left(A^{-1}\right)^{T}$
\end_inset


\end_layout

