\begin_layout Standard
\begin_inset Formula 
\[
at^{2}+bt+c
\]

\end_inset


\end_layout

\begin_layout Standard
נוסחאת ההפרשים הבאה היא:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\Delta_{\text{deg 2 polynom}}\left(t\right) & =f\left(t+\Delta t\right)-f\left(t\right)\\
 & =a\left(t+\Delta t\right)^{2}+b\left(t+\Delta t\right)+c-\left(at^{2}+bt+c\right)\\
 & =a\left(t+\Delta t\right)^{2}+b\left(t+\Delta t\right)-\left(at^{2}+bt\right)\\
 & =a\left(t+\Delta t\right)^{2}+b\left(\Delta t\right)-\left(at^{2}\right)\\
 & =a\left[t^{2}+2t\left(\Delta t\right)+\left(\Delta t\right)^{2}\right]+b\left(\Delta t\right)-\left(at^{2}\right)\\
 & =a\left[2t\left(\Delta t\right)+\left(\Delta t\right)^{2}\right]+b\left(\Delta t\right)\\
 & =2at\left(\Delta t\right)+a\left(\Delta t\right)^{2}+b\left(\Delta t\right)\\
 & =t\left(2a\left(\Delta t\right)\right)+a\left(\Delta t\right)^{2}+b\left(\Delta t\right)
\end{align*}

\end_inset

 נוסחאת ההפרשים מניבה פולינום מדרגה
